<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>


<BorderPane prefHeight="400.0" prefWidth="778.0" style="-fx-background-color: beige;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.A11Controller">
   <bottom>
      <FlowPane alignment="CENTER" prefHeight="65.0" prefWidth="600.0" style="-fx-background-color: #afaaaa;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="cancelBtn" mnemonicParsing="false" onMouseClicked="#cancel" style="-fx-background-radius: 15; -fx-border-radius: 15;" text="BACK">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </bottom>
   <center>
      <TableView fx:id="a11Table" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: beige; -fx-border-color: #afaaaa;" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="sttCol" editable="false" prefWidth="75.0" style="-fx-background-color: beige; -fx-border-color: #afaaaa;" text="STT" />
          <TableColumn fx:id="hotenCol" editable="false" prefWidth="75.0" style="-fx-background-color: beige; -fx-border-color: #afaaaa;" text="HỌ TÊN" />
            <TableColumn fx:id="vitriCol" editable="false" prefWidth="75.0" style="-fx-background-color: beige; -fx-border-color: #afaaaa;" text="VỊ TRÍ ĐẢM NHIỆM" />
        </columns>
         <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </BorderPane.margin>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
</BorderPane>
